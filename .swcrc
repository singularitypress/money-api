{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "importAssertions": true,
      "syntax": "typescript",
      "dynamicImport": false,
      "decorators": false
    },
    "target": "es5",
    "loose": false,
    "externalHelpers": false,
    // Requires v1.2.50 or upper and requires target to be es2016 or upper.
    "keepClassNames": false
  },
  "minify": true,
  "module": {
    "type": "commonjs",
    "strict": false,
    "noInterop": false,
    "lazy": false
  }
}